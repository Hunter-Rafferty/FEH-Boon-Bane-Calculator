    cout<<"HP: This stat is a superboon!"<<endl;
}
else if((lvlFortyStatHP - lvlOneStatHP) == 19){
    cout<<"HP: This stat is a superboon!"<<endl;
}
else if((lvlFortyStatHP - lvlOneStatHP) == 30){
    cout<<"HP: This stat is a superboon!"<<endl;
}
else if((lvlFortyStatHP - lvlOneStatHP) == 13){
    cout<<("HP: This stat is a superbane...")<<endl;
}
else if((lvlFortyStatHP - lvlOneStatHP) == 22){
    cout<<("HP: This stat is a superbane...")<<endl;
}
else if((lvlFortyStatHP - lvlOneStatHP) == 33){
    cout<<("HP: This stat is a superbane...")<<endl;
}
else
cout<<("HP: This stat is the usual +3/-3")<<endl;


if((lvlFortyStatATK - lvlOneStatATK) == 10){
