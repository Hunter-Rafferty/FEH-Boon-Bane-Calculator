int lvlFortyStatDEF;
int lvlFortyStatRES;
int lvlOneStatHP;
int lvlOneStatATK;
int lvlOneStatSPD;
int lvlOneStatDEF;
int lvlOneStatRES;

int main()
{
    cout<<"Insert level 40 HP stat: "<<endl;
    cin>>lvlFortyStatHP;

