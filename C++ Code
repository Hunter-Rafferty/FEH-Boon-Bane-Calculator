    cout<<"SPD: This stat is a superboon!"<<endl;
}
else if((lvlFortyStatSPD - lvlOneStatSPD) == 13){
    cout<<("SPD: This stat is a superbane...")<<endl;
}
else if((lvlFortyStatSPD - lvlOneStatSPD) == 22){
    cout<<("SPD: This stat is a superbane...")<<endl;
